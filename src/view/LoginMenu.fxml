<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="controller.LoginMenuController">
   <children>
      <VBox layoutX="120.0" prefHeight="600.0" prefWidth="260.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="260.0">
               <children>
                  <Label prefHeight="100.0" prefWidth="100.0" text="Username:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="userTxt" prefHeight="60.0" prefWidth="150.0">
                     <HBox.margin>
                        <Insets top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="100.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="260.0">
               <children>
                  <Label prefHeight="100.0" prefWidth="100.0" text="Password:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="passTxt" prefHeight="60.0" prefWidth="150.0">
                     <HBox.margin>
                        <Insets top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="260.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onActionLogin" prefHeight="50.0" prefWidth="80.0" text="Login">
                     <HBox.margin>
                        <Insets left="40.0" top="25.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button layoutX="30.0" layoutY="35.0" mnemonicParsing="false" onAction="#onActionExit" prefHeight="50.0" prefWidth="80.0" text="Exit">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="30.0" top="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="locLbl" alignment="CENTER" prefHeight="100.0" prefWidth="260.0" text="Location of User">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
